<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_background"
    android:orientation="vertical">


    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_2sdp"
            app:cardMaxElevation="@dimen/_3sdp">

            <include
                layout="@layout/match_timer_info"
                android:visibility="visible"></include>

        </android.support.v7.widget.CardView>


        <android.support.design.widget.CoordinatorLayout
            android:id="@+id/coordinatorLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <android.support.design.widget.AppBarLayout
                android:id="@+id/appBarLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:theme="@style/AppTheme.AppBarOverlay"
                app:elevation="0dp">

                <android.support.v7.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:contentInsetStart="0dp"
                    app:layout_scrollFlags="scroll|enterAlways"
                    app:popupTheme="@style/AppTheme.PopupOverlay">


                    <FrameLayout
                        android:id="@+id/fram"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <android.support.v7.widget.CardView
                                android:id="@+id/scorecard"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="@dimen/_8sdp"
                                android:background="@color/white"
                                android:visibility="visible"
                                app:cardElevation="@dimen/_3ssp">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dimen_10dp"
                                    android:orientation="vertical">

                                    <com.mw.eleven11.customView.CustomTextView
                                        android:id="@+id/scorecardCap"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginLeft="@dimen/dimen_10dp"
                                        android:layout_marginRight="@dimen/dimen_10dp"
                                        android:text="@string/scorecardCap"
                                        android:textColor="@color/secondary_text"
                                        android:textSize="@dimen/secondary_txt_size"
                                        app:textfont="sb" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginLeft="@dimen/dimen_10dp"
                                        android:layout_marginRight="@dimen/dimen_10dp"
                                        android:orientation="vertical">

                                        <com.mw.eleven11.customView.CustomTextView
                                            android:id="@+id/scoreData"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_marginTop="@dimen/margin_5"
                                            android:layout_weight="1"
                                            android:text="SA"
                                            android:textColor="@color/primary_text"
                                            android:textSize="@dimen/primary_txt_size"
                                            android:visibility="gone"
                                            app:textfont="bold" />

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_marginTop="@dimen/_4sdp"
                                            android:orientation="horizontal"
                                            android:weightSum="2">

                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                android:orientation="horizontal">

                                                <com.mw.eleven11.customView.CustomTextView

                                                    android:id="@+id/team1"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"

                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                                <com.mw.eleven11.customView.CustomTextView

                                                    android:id="@+id/score1"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"
                                                    android:layout_alignRight="@id/team1"
                                                    android:layout_alignParentTop="true"
                                                    android:layout_alignParentBottom="true"
                                                    android:layout_marginLeft="@dimen/dimen_30dp"
                                                    android:layout_marginBottom="0dp"

                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                                <com.mw.eleven11.customView.CustomTextView

                                                    android:id="@+id/overs1"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"

                                                    android:layout_marginLeft="@dimen/margin_5"
                                                    android:textColor="@color/secondary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"
                                                    android:layout_marginLeft="@dimen/margin_20"
                                                    android:text="|"
                                                    android:textColor="@color/black"
                                                    app:textfont="@string/MONTSERRAT_REGULAR" />


                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                android:orientation="horizontal">


                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/team2"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"


                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/score2"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"
                                                    android:layout_marginLeft="@dimen/dimen_30dp"

                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/overs2"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"

                                                    android:layout_marginLeft="@dimen/margin_5"
                                                    android:textColor="@color/secondary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                            </LinearLayout>

                                        </LinearLayout>

                                    </LinearLayout>

                                    <com.mw.eleven11.customView.CustomTextView
                                        android:id="@+id/liveStatus"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/dimen_10dp"
                                        android:layout_marginTop="@dimen/_4sdp"
                                        android:layout_marginRight="@dimen/dimen_10dp"
                                        android:layout_weight="1"
                                        android:hint="SA"
                                        android:textColor="@color/primary_text"
                                        android:textSize="@dimen/secondary_txt_size"
                                        app:textfont="r" />


                                    <com.mw.eleven11.customView.CustomTextView
                                        android:id="@+id/viewPlayerStates"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_4sdp"
                                        android:layout_weight="1"
                                        android:background="#EEEEEE"
                                        android:drawablePadding="@dimen/_4sdp"
                                        android:gravity="center_vertical"
                                        android:padding="@dimen/_4sdp"
                                        android:text="@string/viewPlayerStates"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/textsize_small"
                                        app:drawableLeftCompat="@drawable/ic_player_state_black"
                                        app:drawableRightCompat="@drawable/ic_arrow_black_new"
                                        app:textfont="r" />

                                </LinearLayout>
                            </android.support.v7.widget.CardView>

                            <android.support.v7.widget.CardView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dimen_10dp"
                                android:layout_marginTop="@dimen/dimen_10dp"
                                android:layout_marginEnd="@dimen/dimen_10dp"
                                android:layout_marginRight="@dimen/dimen_10dp"
                                android:background="@color/backgroundMain"
                                android:visibility="gone"
                                app:cardElevation="@dimen/cardElvationMore"
                                app:contentPadding="@dimen/dimen_10dp">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:weightSum="3">

                                        <ImageView
                                            android:id="@+id/free_icon"
                                            android:layout_width="@dimen/dimen_40dp"
                                            android:layout_height="@dimen/dimen_40dp"
                                            android:padding="@dimen/margin_5"
                                            android:visibility="gone"
                                            app:srcCompat="@drawable/ic_free_sticker" />

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:orientation="vertical">

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/total_winnings"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:text="@string/price_pool"
                                                android:textColor="@color/warm_grey"
                                                android:textSize="@dimen/textsize_too_small"
                                                app:textAllCaps="true"
                                                app:textfont="light" />

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/ctv_winning_amount"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/margin_5"
                                                android:text="15 Lakhs"

                                                android:textColor="@color/black"
                                                android:textSize="@dimen/textsize_small"
                                                app:drawableLeftCompat="@drawable/ic_rupee"
                                                app:textfont="@string/MONTSERRAT_BOLD" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:gravity="center_horizontal"
                                            android:orientation="vertical">

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/winners"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:text="@string/winners"
                                                android:textColor="@color/warm_grey"
                                                android:textSize="@dimen/textsize_too_small"
                                                app:textfont="light" />

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/rl_winnings"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/margin_5"

                                                android:drawablePadding="@dimen/margin_5"
                                                android:text="6000"
                                                android:textColor="@color/contest_blue"
                                                android:textSize="@dimen/textsize_small"
                                                app:drawableRightCompat="@drawable/ic_down_arrow"
                                                app:textfont="@string/MONTSERRAT_BOLD" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:orientation="vertical">

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/entry_fee"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:text="@string/entry_fee"
                                                android:textColor="@color/warm_grey"
                                                android:textSize="@dimen/textsize_too_small"
                                                app:textfont="light" />

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/ctv_entry_fee"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/margin_5"
                                                android:drawableTint="@color/doneIconColor"

                                                android:text="499"
                                                android:textColor="@color/doneIconColor"
                                                android:textSize="@dimen/textsize_small"
                                                app:drawableLeftCompat="@drawable/ic_rupee"
                                                app:textfont="light" />

                                        </LinearLayout>
                                    </LinearLayout>

                                    <View
                                        android:id="@+id/view1"
                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"

                                        android:layout_marginTop="@dimen/dimen_10dp"
                                        android:background="@color/line_unselect" />

                                    <LinearLayout
                                        android:id="@+id/switchTeam_lin"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical"
                                        android:visibility="gone">

                                        <LinearLayout

                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_margin="@dimen/margin_5"
                                            android:orientation="horizontal"
                                            android:weightSum="3">

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent"
                                                android:layout_weight="2"
                                                android:orientation="vertical">

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/joinedTeam"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@string/joinedTeam"
                                                    android:textColor="@color/warm_grey"
                                                    android:textSize="@dimen/textsize_too_small"
                                                    app:textfont="light" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/teamName"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginTop="@dimen/margin_5"

                                                    android:text="Team 1"
                                                    android:textColor="@color/contest_blue"
                                                    android:textSize="@dimen/textsize_small" />

                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:gravity="center_vertical">

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/ctvSwitchTeam"
                                                    android:layout_width="110dp"
                                                    android:layout_height="wrap_content"
                                                    android:background="@drawable/join_button_bg"
                                                    android:gravity="center"
                                                    android:padding="@dimen/margin_5"
                                                    android:paddingLeft="@dimen/margin_20"
                                                    android:paddingRight="@dimen/margin_20"
                                                    android:text="@string/switchTeam"
                                                    android:textAllCaps="true"
                                                    android:textColor="@color/contest_blue"
                                                    android:textSize="@dimen/textsize_too_small"
                                                    app:textfont="@string/MONTSERRAT_REGULAR" />

                                            </LinearLayout>
                                        </LinearLayout>

                                        <View
                                            android:layout_width="match_parent"
                                            android:layout_height="1dp"

                                            android:layout_marginTop="@dimen/dimen_10dp"
                                            android:background="@color/line_unselect" />
                                    </LinearLayout>


                                    <LinearLayout
                                        android:id="@+id/linout"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:visibility="gone"
                                        android:weightSum="1">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/margin_5"
                                            android:orientation="vertical">

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/confirmWinningTag"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/margin_5"
                                                android:drawablePadding="@dimen/margin_5"
                                                android:text="@string/confirmWinningTag"
                                                android:textColor="@color/warm_grey"
                                                android:textSize="@dimen/textsize_small"

                                                app:drawableLeftCompat="@drawable/confirmed" />

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/joinMultipleTag"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/margin_5"
                                                android:drawablePadding="@dimen/margin_5"
                                                android:text="@string/joinMultipleTag"

                                                android:textColor="@color/warm_grey"
                                                android:textSize="@dimen/textsize_small"
                                                app:drawableLeftCompat="@drawable/multipal" />

                                            <ProgressBar
                                                android:id="@+id/progress_view"
                                                style="@android:style/Widget.ProgressBar.Horizontal"
                                                android:layout_width="match_parent"
                                                android:layout_height="4dp"
                                                android:layout_centerHorizontal="true"
                                                android:layout_centerVertical="true"

                                                android:layout_marginTop="10dp"

                                                android:indeterminate="false"
                                                android:max="100"
                                                android:maxHeight="10dp"
                                                android:minHeight="10dp"
                                                android:progress="24"
                                                android:progressDrawable="@drawable/progress_drawable" />

                                            <RelativeLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent"

                                                android:layout_marginTop="@dimen/margin_5">

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/spotLeftCount"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_alignParentStart="true"
                                                    android:layout_alignParentLeft="true"
                                                    android:text="Only 81,4232 spot left"
                                                    android:textColor="@color/textColor"
                                                    android:textSize="9sp" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/teamsCount"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_alignParentEnd="true"
                                                    android:layout_alignParentRight="true"
                                                    android:text="1,33,333 spots"
                                                    android:textColor="@color/textColor"
                                                    android:textSize="9sp" />
                                            </RelativeLayout>


                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/ctv_join"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/element_margin_top"

                                                android:layout_marginBottom="@dimen/element_margin_top"
                                                android:background="@drawable/shedow_green_background"
                                                android:gravity="center"
                                                android:padding="@dimen/dimen_10dp"
                                                android:text="@string/joinThisContest"
                                                android:textAllCaps="true"
                                                android:textColor="@color/white"

                                                android:textSize="@dimen/textsize_small"
                                                android:visibility="gone" />


                                        </LinearLayout>

                                    </LinearLayout>


                                </LinearLayout>
                            </android.support.v7.widget.CardView>


                            <LinearLayout
                                android:id="@+id/upcomingMatchesLin"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <com.mw.eleven11.customView.CustomTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/noContestJoined"
                                        android:textAlignment="center"
                                        android:textColor="@color/primary_text"
                                        android:textSize="@dimen/primary_txt_size"
                                        android:visibility="visible"
                                        app:textfont="sb" />

                                    <com.mw.eleven11.customView.CustomTextView
                                        android:id="@+id/joinContestUpcoming"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textAlignment="center"
                                        android:textColor="@color/primary_text"
                                        android:textSize="@dimen/primary_txt_size"
                                        android:visibility="visible"
                                        app:textfont="sb"
                                        tools:text="AA vs BB" />
                                </LinearLayout>

                                <com.mw.eleven11.customView.CustomTextView

                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/margin_5"
                                    android:layout_marginBottom="@dimen/dimen_10dp"
                                    android:text="@string/joinContestUpcoming"
                                    android:textAlignment="center"
                                    android:textColor="@color/primary_text"
                                    android:textSize="@dimen/secondary_txt_size"
                                    app:textfont="r" />


                            </LinearLayout>

                        </LinearLayout>

                    </FrameLayout>


                </android.support.v7.widget.Toolbar>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/colorPrimaryDark"
                    android:padding="6dp"
                    android:visibility="gone">

                    <com.mw.eleven11.customView.CustomTextView
                        style="@style/rippleStyleTransparent"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="@dimen/dimen_5dp"
                        android:text="@string/team"
                        android:textAllCaps="true"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/text_medium"
                        app:rd_enable="false"
                        app:textfont="light" />

                    <com.mw.eleven11.customView.CustomTextView
                        style="@style/rippleStyleTransparent"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/dimen_5dp"
                        android:text="@string/rank"
                        android:textAllCaps="true"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/text_medium"
                        app:rd_enable="false"
                        app:textfont="light" />
                </RelativeLayout>

            </android.support.design.widget.AppBarLayout>

            <android.support.v4.view.ViewPager
                android:id="@+id/viewPager"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                app:layout_behavior="@string/appbar_scrolling_view_behavior" />

        </android.support.design.widget.CoordinatorLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/dreamteamLin"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_60dp"
            android:orientation="horizontal"
            android:visibility="gone"
            android:weightSum="2">

            <LinearLayout
                android:id="@+id/dreamTeam"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/colorPrimaryDark"
                android:gravity="center">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/dimen_20dp"
                        android:layout_height="@dimen/dimen_20dp"
                        app:srcCompat="@drawable/ic_sticker"

                        />

                    <com.mw.eleven11.customView.CustomTextView

                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"

                        android:layout_marginLeft="@dimen/margin_5"
                        android:background="@color/colorPrimaryDark"
                        android:drawablePadding="@dimen/margin_5"
                        android:gravity="center_vertical"
                        android:text="@string/dreamTeam"
                        android:textColor="@color/white"
                        android:textSize="@dimen/textsize_small"
                        app:drawableLeftCompat="@drawable/ic_sticker"
                        app:textfont="@string/MONTSERRAT_REGULAR"

                        />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/tellYourFriends"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/colorPrimary"
                android:gravity="center">

                <com.mw.eleven11.customView.CustomTextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/dimen_10dp"
                    android:gravity="center_vertical"
