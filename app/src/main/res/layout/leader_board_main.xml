<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_background">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_4sdp"
            app:cardMaxElevation="@dimen/_3sdp">

            <include
                layout="@layout/match_timer_info"
                android:visibility="visible"></include>

        </android.support.v7.widget.CardView>


        <android.support.design.widget.CoordinatorLayout
            android:id="@+id/coordinatorLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

            <android.support.design.widget.AppBarLayout
                android:id="@+id/appBarLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:theme="@style/AppTheme.AppBarOverlay"
                app:elevation="0dp">

                <android.support.v7.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="@dimen/_8sdp"
                    app:contentInsetStart="0dp"
                    app:layout_scrollFlags="scroll|enterAlways"
                    app:popupTheme="@style/AppTheme.PopupOverlay">


                    <FrameLayout
                        android:id="@+id/fram"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/default_background">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <com.mw.eleven11.customView.CustomTextView
                                android:id="@+id/contestTypeName"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/text_small"
                                android:layout_marginTop="@dimen/margin_5"
                                android:gravity="center"
                                android:padding="@dimen/margin_5"
                                android:textColor="@color/black"
                                android:textSize="@dimen/text_medium"
                                android:visibility="gone"
                                app:textAllCaps="true"
                                app:textfont="@string/bold" />

                            <android.support.v7.widget.CardView
                                android:id="@+id/scorecard"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/_8sdp"
                                android:layout_marginRight="@dimen/_8sdp"
                                android:layout_marginBottom="@dimen/_8sdp"
                                android:background="@color/white"
                                android:visibility="visible"
                                app:cardElevation="@dimen/_3ssp">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dimen_10dp"
                                    android:orientation="vertical">

                                    <com.mw.eleven11.customView.CustomTextView
                                        android:id="@+id/scorecardCap"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginLeft="@dimen/dimen_10dp"
                                        android:layout_marginRight="@dimen/dimen_10dp"
                                        android:text="@string/scorecardCap"
                                        android:textColor="@color/secondary_text"
                                        android:textSize="@dimen/secondary_txt_size"
                                        app:textfont="sb" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginLeft="@dimen/dimen_10dp"
                                        android:layout_marginRight="@dimen/dimen_10dp"
                                        android:orientation="vertical">

                                        <com.mw.eleven11.customView.CustomTextView
                                            android:id="@+id/scoreData"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_marginTop="@dimen/margin_5"
                                            android:layout_weight="1"
                                            android:text="SA"
                                            android:textColor="@color/primary_text"
                                            android:textSize="@dimen/primary_txt_size"
                                            android:visibility="gone"
                                            app:textfont="bold" />

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_marginTop="@dimen/_4sdp"
                                            android:orientation="horizontal"
                                            android:weightSum="2">

                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                android:orientation="horizontal">

                                                <com.mw.eleven11.customView.CustomTextView

                                                    android:id="@+id/team1"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"

                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                                <com.mw.eleven11.customView.CustomTextView

                                                    android:id="@+id/score1"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"
                                                    android:layout_alignRight="@id/team1"
                                                    android:layout_alignParentTop="true"
                                                    android:layout_alignParentBottom="true"
                                                    android:layout_marginLeft="@dimen/dimen_30dp"
                                                    android:layout_marginBottom="0dp"

                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                                <com.mw.eleven11.customView.CustomTextView

                                                    android:id="@+id/overs1"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"

                                                    android:layout_marginLeft="@dimen/margin_5"
                                                    android:textColor="@color/secondary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"
                                                    android:layout_marginLeft="@dimen/margin_20"
                                                    android:text="|"
                                                    android:textColor="@color/black"
                                                    app:textfont="@string/MONTSERRAT_REGULAR" />


                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                android:orientation="horizontal">


                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/team2"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"


                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/score2"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"
                                                    android:layout_marginLeft="@dimen/dimen_30dp"

                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/overs2"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"

                                                    android:layout_marginLeft="@dimen/margin_5"
                                                    android:textColor="@color/secondary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    android:visibility="visible"
                                                    app:textfont="r"
                                                    tools:text="SA" />

                                            </LinearLayout>

                                        </LinearLayout>

                                    </LinearLayout>

                                    <com.mw.eleven11.customView.CustomTextView
                                        android:id="@+id/liveStatus"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/dimen_10dp"
                                        android:layout_marginTop="@dimen/_4sdp"
                                        android:layout_marginRight="@dimen/dimen_10dp"
                                        android:layout_weight="1"
                                        android:hint="SA"
                                        android:textColor="@color/primary_text"
                                        android:textSize="@dimen/secondary_txt_size"
                                        app:textfont="r" />


                                    <com.mw.eleven11.customView.CustomTextView
                                        android:visibility="gone"
                                        android:id="@+id/viewPlayerStates"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_4sdp"
                                        android:layout_weight="1"
                                        android:background="#EEEEEE"
                                        android:drawablePadding="@dimen/_4sdp"
                                        android:gravity="center_vertical"
                                        android:padding="@dimen/_4sdp"
                                        android:text="@string/viewPlayerStates"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/textsize_small"
                                        app:drawableLeftCompat="@drawable/ic_player_state_black"
                                        app:drawableRightCompat="@drawable/ic_arrow_black_new"
                                        app:textfont="r" />

                                </LinearLayout>
                            </android.support.v7.widget.CardView>

                            <android.support.v7.widget.CardView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/_8sdp"
                                android:layout_marginRight="@dimen/_8sdp"
                                android:layout_marginBottom="@dimen/_8sdp"

                                app:cardElevation="@dimen/_3ssp"
                                app:contentPadding="@dimen/dimen_10dp">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="@dimen/_4sdp"
                                        android:weightSum="3">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:orientation="vertical">

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/total_winnings"

                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:includeFontPadding="false"
                                                android:text="@string/price_pool"
                                                android:textColor="@color/secondary_text"
                                                android:textSize="@dimen/secondary_txt_size"
                                                app:textAllCaps="true"
                                                app:textfont="sb" />

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/ctv_winning_amount"

                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:includeFontPadding="false"
                                                android:textColor="@color/primary_text"
                                                android:textSize="@dimen/primary_txt_size"
                                                app:textfont="r"
                                                tools:text="15 Lakhs" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:visibility="invisible"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:gravity="center_horizontal"
                                            android:orientation="vertical">

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/winners"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:includeFontPadding="false"
                                                android:text="@string/winners"
                                                android:textColor="@color/secondary_text"

                                                android:textSize="@dimen/secondary_txt_size"
                                                app:textfont="sb" />

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/rl_winnings"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:background="@color/colorPrimaryLight"
                                                android:drawablePadding="@dimen/_8sdp"
                                                android:drawableTint="@color/primary_text"
                                                android:gravity="center"
                                                android:includeFontPadding="false"
                                                android:paddingLeft="@dimen/_10ssp"
                                                android:paddingRight="@dimen/_10ssp"
                                                android:text="6000"
                                                android:textColor="@color/primary_text"
                                                android:textSize="@dimen/secondary_txt_size"
                                                app:drawableRightCompat="@drawable/ic_down_arrow"
                                                app:textfont="r" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:gravity="end"
                                            android:orientation="vertical">

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/entry_fee"
                                                android:layout_width="@dimen/dimen_60dp"
                                                android:layout_height="wrap_content"
                                                android:includeFontPadding="false"
                                                android:text="@string/entry_fee"
                                                android:textAlignment="center"
                                                android:textColor="@color/secondary_text"
                                                android:textSize="@dimen/secondary_txt_size"
                                                app:textfont="sb" />

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/ctv_entry_fee"
                                                android:layout_width="@dimen/dimen_60dp"
                                                android:layout_height="wrap_content"
                                                android:includeFontPadding="false"
                                                android:textAlignment="center"
                                                android:textColor="@color/primary_text"
                                                android:textSize="@dimen/primary_txt_size"
                                                app:textfont="r"
                                                tools:text="499" />

                                        </LinearLayout>
                                    </LinearLayout>


                                    <LinearLayout
                                        android:id="@+id/switchTeam_lin"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                        <View
                                            android:id="@+id/view1"
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/_1sdp"
                                            android:layout_marginTop="@dimen/_4sdp"

                                            android:layout_marginBottom="@dimen/_8sdp"
                                            android:background="@color/default_background" />

                                        <LinearLayout

                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <LinearLayout
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:orientation="vertical">


                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/joinedTeam"

                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:includeFontPadding="false"
                                                    android:text="@string/joinedTeam"
                                                    android:textColor="@color/secondary_text"
                                                    android:textSize="@dimen/secondary_txt_size"
                                                    app:textAllCaps="true"
                                                    app:textfont="sb" />


                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/teamName"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:includeFontPadding="false"
                                                    android:text="Team 1"
                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/primary_txt_size"
                                                    app:textfont="r"
                                                    tools:text="15 Lakhs" />


                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="match_parent"
                                                android:gravity="center_vertical">

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/ctvSwitchTeam"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:background="@drawable/button_empty_bg_white_stroke_theme"
                                                    android:gravity="center"
                                                    android:paddingLeft="@dimen/_16sdp"
                                                    android:paddingTop="@dimen/_2sdp"
                                                    android:paddingRight="@dimen/_16sdp"
                                                    android:paddingBottom="@dimen/_2sdp"
                                                    android:text="@string/switchTeam"
                                                    android:textAllCaps="true"
                                                    android:textColor="@color/white"
                                                    android:textSize="@dimen/secondary_txt_size"
                                                    app:textfont="r" />

                                            </LinearLayout>
                                        </LinearLayout>

                                        <View
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/_1sdp"
                                            android:layout_marginTop="@dimen/_6sdp"
                                            android:layout_marginBottom="@dimen/_6sdp"
                                            android:background="@color/default_background" />

                                    </LinearLayout>


                                    <LinearLayout
                                        android:id="@+id/linout"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:weightSum="1">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">

                                            <LinearLayout
                                                android:id="@+id/confirmWinningTagRoot"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:gravity="center_vertical"
                                                android:orientation="horizontal">

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:layout_width="@dimen/_12sdp"
                                                    android:layout_height="@dimen/_12sdp"
                                                    android:layout_marginRight="@dimen/_4sdp"
                                                    android:background="@drawable/bg_multi"
                                                    android:gravity="center"
                                                    android:text="C"
                                                    android:textColor="@color/secondary_text"
                                                    android:textSize="@dimen/_7ssp"
                                                    app:textfont="sb" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/confirmWinningTag"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@string/confirmWinningTag"
                                                    android:textColor="@color/secondary_text"
                                                    android:textSize="@dimen/secondary_txt_size"
                                                    app:textfont="r" />


                                            </LinearLayout>


                                            <LinearLayout
                                                android:id="@+id/joinMultipleTagRoot"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/_4sdp"
                                                android:gravity="center_vertical"
                                                android:orientation="horizontal">

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:layout_width="@dimen/_12sdp"
                                                    android:layout_height="@dimen/_12sdp"
                                                    android:layout_marginRight="@dimen/_4sdp"
                                                    android:background="@drawable/bg_multi"
                                                    android:gravity="center"
                                                    android:text="M"
                                                    android:textColor="@color/secondary_text"
                                                    android:textSize="@dimen/_7ssp"
                                                    app:textfont="sb" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/joinMultipleTag"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@string/joinMultipleTag"
                                                    android:textColor="@color/secondary_text"
                                                    android:textSize="@dimen/secondary_txt_size"
                                                    app:textfont="r" />


                                            </LinearLayout>


                                            <ProgressBar
                                                android:id="@+id/progress_view"
                                                style="@android:style/Widget.ProgressBar.Horizontal"
                                                android:layout_width="match_parent"
                                                android:layout_height="@dimen/_4sdp"
                                                android:layout_centerHorizontal="true"
                                                android:layout_centerVertical="true"
                                                android:layout_marginTop="@dimen/_4sdp"
                                                android:indeterminate="false"
                                                android:max="100"
                                                android:progress="24"
                                                android:progressDrawable="@drawable/progress_drawable" />

                                            <RelativeLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent"
                                                android:layout_marginTop="@dimen/_4sdp">

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/spotLeftCount"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_alignParentStart="true"
                                                    android:layout_alignParentLeft="true"
                                                    android:text="Only 81,4232 spots left"
                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/secondary_txt_size"
                                                    app:textfont="r" />

                                                <com.mw.eleven11.customView.CustomTextView
                                                    android:id="@+id/teamsCount"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_alignParentEnd="true"
                                                    android:layout_alignParentRight="true"
                                                    android:text="1,33,333 Spots"
                                                    android:textColor="@color/primary_text"
                                                    android:textSize="@dimen/secondary_txt_size"
                                                    app:textfont="r" />

                                            </RelativeLayout>

                                            <com.mw.eleven11.customView.CustomTextView
                                                android:id="@+id/ctv_join"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/_12sdp"
                                                android:layout_marginBottom="@dimen/_12sdp"
                                                android:background="@drawable/button_solid_bg_theme_active"
                                                android:gravity="center"
                                                android:padding="@dimen/_4sdp"
                                                android:textAllCaps="true"
                                                android:textColor="@color/white"
                                                android:textSize="@dimen/primary_txt_size"
                                                android:visibility="gone"
                                                app:textfont="sb"
                                                tools:text="@string/joinThisContest" />


                                        </LinearLayout>

                                    </LinearLayout>


                                </LinearLayout>
                            </android.support.v7.widget.CardView>


                        </LinearLayout>

                    </FrameLayout>


                </android.support.v7.widget.Toolbar>


                <android.support.design.widget.TabLayout
                    android:layout_marginBottom="@dimen/_2sdp"
                    android:id="@+id/tab_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_32sdp"
                    android:background="@color/white"
                    android:backgroundTint="@color/white"
                    app:tabGravity="fill"
                    app:tabIndicatorColor="@color/black"
                    app:tabIndicatorFullWidth="true"
                    app:tabIndicatorHeight="2dp"
                    app:tabMode="fixed"
                    app:tabTextColor="@color/black" />


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/colorPrimary"
                    android:paddingLeft="@dimen/_8sdp"
                    android:paddingTop="@dimen/_6sdp"
                    android:paddingRight="@dimen/_8sdp"
                    android:paddingBottom="@dimen/_6sdp"
                    android:visibility="gone">

                    <com.mw.eleven11.customView.CustomTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/team"
                        android:textColor="@color/white"
                        android:textSize="@dimen/secondary_txt_size"
                        app:textfont="r" />

                    <com.mw.eleven11.customView.CustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:text="@string/rank"
                        android:textColor="@color/white"
                        android:textSize="@dimen/secondary_txt_size"
                        app:textfont="r" />
                </RelativeLayout>

            </android.support.design.widget.AppBarLayout>

            <android.support.v4.view.ViewPager
                android:id="@+id/viewPager"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/default_background"
                app:layout_behavior="@string/appbar_scrolling_view_behavior" />

        </android.support.design.widget.CoordinatorLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/dreamteamLin"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/default_background"
        android:padding="@dimen/_8sdp"
        android:visibility="gone">


        <com.mw.eleven11.customView.CustomTextView
            android:id="@+id/dreamTeam"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:background="@drawable/button_empty_bg_white_stroke_theme"
            android:paddingLeft="@dimen/_32sdp"
            android:paddingTop="@dimen/_4sdp"
            android:paddingRight="@dimen/_32sdp"
            android:paddingBottom="@dimen/_4sdp"
            android:text="@string/dreamTeam"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="@dimen/_12ssp"
            app:textfont="r" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/header_background_home_page"
            android:gravity="center"
            android:orientation="horizontal"
            android:visibility="gone">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_sticker" />

            <com.mw.eleven11.customView.CustomTextView

